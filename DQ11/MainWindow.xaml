<Window x:Class="DQ11.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DQ11"
        mc:Ignorable="d"
        Title="DQ11 セーブデータ改造(Nintendo 3DS)" Height="650" Width="800"
        WindowStartupLocation="CenterScreen"
        AllowDrop="True" Drop="Window_Drop" PreviewDragOver="Window_PreviewDragOver"
        Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="0">
            <MenuItem Header="ファイル(_F)">
                <MenuItem x:Name="MenuItemFileOpen" Header="開く(_O)" Click="MenuItemFileOpen_Click"/>
                <MenuItem x:Name="MenuItemFileOpenForce" Header="強制的に開く(_R)" Click="MenuItemFileOpenForce_Click"/>
                <MenuItem x:Name="MenuItemFileSave" Header="上書き保存(_S)" Click="MenuItemFileSave_Click"/>
                <MenuItem x:Name="MenuItemFileSaveAs" Header="名前を付けて保存(_S)" Click="MenuItemFileSaveAs_Click"/>
                <Separator/>
                <MenuItem x:Name="MenuItemExit" Header="終了(_X)" Click="MenuItemExit_Click"/>
            </MenuItem>
            <MenuItem x:Name="MenuItemAbout" Header="情報(_A)" Click="MenuItemAbout_Click"/>
        </Menu>
        <ToolBar Grid.Row="1">
            <Button x:Name="ToolBarFileOpen" Width="32" Height="32" Click="ToolBarFileOpen_Click" ToolTip="開く">
                <Image Source="{StaticResource Open}"/>
            </Button>
            <Button x:Name="ToolBarFileSave" Width="32" Height="32" Click="ToolBarFileSave_Click" ToolTip="保存">
                <Image Source="{StaticResource Save}"/>
            </Button>
        </ToolBar>
        <TabControl Grid.Row="2">
            <TabItem Header="基本設定">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="140"/>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="40"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Label Content="プレイ時間" Grid.RowSpan="3" VerticalAlignment="Center"/>
                        <Label Content="時間" Grid.Column="2"/>
                        <Label Content="分"  Grid.Row="1" Grid.Column="2"/>
                        <Label Content="秒"  Grid.Row="2" Grid.Column="2"/>
                        <Label Content="もち金" Grid.Row="3"/>
                        <Label Content="G" Grid.Row="3" Grid.Column="2"/>
                        <Label Content="銀行" Grid.Row="4"/>
                        <Label Content="G" Grid.Row="4" Grid.Column="2"/>
                        <Label Content="カジノコイン" Grid.Row="5"/>
                        <Label Content="枚" Grid.Row="5" Grid.Column="2"/>
                        <Label Content="ちいさなメダル" Grid.Row="6"/>
                        <Label Content="枚" Grid.Row="6" Grid.Column="2"/>
                        <Label Content="あずけたメダル" Grid.Row="7"/>
                        <Label Content="枚" Grid.Row="7" Grid.Column="2"/>
                        <Label Content="うちなおしの宝珠" Grid.Row="8"/>
                        <Label Content="こ" Grid.Row="8" Grid.Column="2"/>
                        <Label Content="乗り物" Grid.Row="10"/>
                        <Label Content="クリア" Grid.Row="11"/>
                        <TextBox Grid.Column="1" Text="{Binding PlayHour, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding PlayMinute, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="2" Grid.Column="1" Text="{Binding PlaySecond, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="3" Grid.Column="1" Text="{Binding GoldHand, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="4" Grid.Column="1" Text="{Binding GoldBank, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="5" Grid.Column="1" Text="{Binding CasinoCoin, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="6" Grid.Column="1" Text="{Binding SmallMedal, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="7" Grid.Column="1" Text="{Binding DepositSmallMedal, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="8" Grid.Column="1" Text="{Binding RebuildJewel, UpdateSourceTrigger=PropertyChanged}"/>
                        <ComboBox Grid.Row="10" Grid.Column="1" SelectedIndex="{Binding Vehicle}">
                            <ComboBoxItem Content="無し"/>
                            <ComboBoxItem Content="馬"/>
                            <ComboBoxItem Content="パールモービル"/>
                            <ComboBoxItem Content="スカルライダー"/>
                            <ComboBoxItem Content="ビーライダー"/>
                            <ComboBoxItem Content="ドラゴンライダー"/>
                            <ComboBoxItem Content="デュラハンナイト"/>
                            <ComboBoxItem Content="バロンナイト"/>
                            <ComboBoxItem Content="船"/>
                        </ComboBox>
                        <CheckBox Grid.Row="11" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding GameClear}"/>
                    </Grid>
                    <Grid Grid.Column="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="140"/>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="30"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Label Content="戦闘勝利回数"/>
                        <Label Content="回" Grid.Column="2"/>
                        <Label Content="ふしぎな鍛冶をした回数" Grid.Row="1"/>
                        <Label Content="回" Grid.Row="1" Grid.Column="2"/>
                        <Label Content="トータル獲得ゴールド" Grid.Row="2"/>
                        <Label Content="G" Grid.Row="2" Grid.Column="2"/>
                        <Label Content="トータル獲得ちいさなメダル" Grid.Row="3"/>
                        <Label Content="枚" Grid.Row="3" Grid.Column="2"/>
                        <Label Content="ツボ・タル破壊" Grid.Row="4"/>
                        <Label Content="回" Grid.Row="4" Grid.Column="2"/>
                        <Label Content="キャンプ" Grid.Row="5"/>
                        <Label Content="回" Grid.Row="5" Grid.Column="2"/>
                        <Label Content="宿屋に泊まった回数" Grid.Row="6"/>
                        <Label Content="回" Grid.Row="6" Grid.Column="2"/>
                        <Label Content="れんけいを使用した回数" Grid.Row="7"/>
                        <Label Content="回" Grid.Row="7" Grid.Column="2"/>
                        <Label Content="スロットを遊んだ回数" Grid.Row="9"/>
                        <Label Content="回" Grid.Row="9" Grid.Column="2"/>
                        <Label Content="ポーカーを遊んだ回数" Grid.Row="10"/>
                        <Label Content="回" Grid.Row="10" Grid.Column="2"/>
                        <Label Content="ルーレットを遊んだ回数" Grid.Row="11"/>
                        <Label Content="回" Grid.Row="11" Grid.Column="2"/>
                        <TextBox Grid.Column="1" Text="{Binding KillMonster, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding Smith, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="2" Grid.Column="1" Text="{Binding TotalGold, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="3" Grid.Column="1" Text="{Binding TotalSmallMedal, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="4" Grid.Column="1" Text="{Binding Break, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="5" Grid.Column="1" Text="{Binding Camp, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="6" Grid.Column="1" Text="{Binding INI, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="7" Grid.Column="1" Text="{Binding Cooperation, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="9" Grid.Column="1" Text="{Binding Slot, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="10" Grid.Column="1" Text="{Binding Poker, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="11" Grid.Column="1" Text="{Binding Roulette, UpdateSourceTrigger=PropertyChanged}"/>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="人物">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="200"/>
                        <ColumnDefinition Width="200"/>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <ListBox x:Name="ListBoxChar" ItemsSource="{Binding Char}" DisplayMemberPath="Name"/>
                    <Grid Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="80"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Label Content="名前"/>
                        <Label Content="Lv" Grid.Row="1"/>
                        <Label Content="経験値" Grid.Row="2"/>
                        <Label Content="現在HP" Grid.Row="3"/>
                        <Label Content="現在MP" Grid.Row="4"/>
                        <Label Content="最大HP+" Grid.Row="5"/>
                        <Label Content="最大MP+" Grid.Row="6"/>
                        <Label Content="こうげき魔力+" Grid.Row="7"/>
                        <Label Content="かいふく魔力+" Grid.Row="8"/>
                        <Label Content="ちから+" Grid.Row="9"/>
                        <Label Content="みのまもり+" Grid.Row="10"/>
                        <Label Content="すばやさ+" Grid.Row="11"/>
                        <Label Content="きようさ+" Grid.Row="12"/>
                        <Label Content="みりょく+" Grid.Row="13"/>
                        <Label Content="スキル＋" Grid.Row="14"/>
                        <Label Content="作戦" Grid.Row="15"/>
                        <Label Content="ゾーン" Grid.Row="16"/>
                        <TextBox Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.Name ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.Lv ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="2" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.Exp ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="3" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.HP ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="4" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.MP ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="5" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.MaxHP ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="6" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.MaxMP ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="7" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.AttackMagic ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="8" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.HealMagic ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="9" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.Attack ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="10" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.Diffence ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="11" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.Speed ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="12" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.Skillful ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="13" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.Charm ,UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Row="14" Grid.Column="1" Text="{Binding ElementName=ListBoxChar, Path=SelectedItem.Skill ,UpdateSourceTrigger=PropertyChanged}"/>
                        <ComboBox x:Name="ComboBoxCharStrategy" Grid.Row="15" Grid.Column="1" SelectedIndex="{Binding ElementName=ListBoxChar, Path=SelectedItem.Strategy}">
                            <ComboBoxItem Content="ガンガンいこうぜ"/>
                            <ComboBoxItem Content="バッチリがんばれ"/>
                            <ComboBoxItem Content="いろいろやろうぜ"/>
                            <ComboBoxItem Content="いのちだいじに"/>
                            <ComboBoxItem Content="MPつかうな"/>
                            <ComboBoxItem Content="めいれいさせろ"/>
                        </ComboBox>
                        <CheckBox Grid.Row="16" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding ElementName=ListBoxChar, Path=SelectedItem.Zone}"/>
                    </Grid>
                    <Grid Grid.Column="3">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition Height="35"/>
                            <RowDefinition Height="100"/>
                        </Grid.RowDefinitions>
                        <ListBox x:Name="ListBoxCharItem" ItemsSource="{Binding ElementName=ListBoxChar, Path=SelectedItem.Item}">
                            <ListBox.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <Button x:Name="ButtonCharItemChange" Content=" ... " Click="ButtonCharItemChange_Click"/>
                                        <Label Content="{Binding Name}"/>
                                    </StackPanel>
                                </DataTemplate>
                            </ListBox.ItemTemplate>
                        </ListBox>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Button Content="最弱" Margin="2,2,2,2" Click="ButtonCharStatusMin_Click"/>
                            <Button Grid.Column="1" Content="最強" Margin="2,2,2,2" Click="ButtonCharStatusMax_Click"/>
                        </Grid>
                        <Grid Grid.Row="2">
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Label Content="装備"/>
                            <Label Content="右手" Grid.Row="1"/>
                            <Label Content="左手" Grid.Row="1" Grid.Column="2"/>
                            <Label Content="頭" Grid.Row="2"/>
                            <Label Content="体" Grid.Row="2" Grid.Column="2"/>
                            <Label Content="アクセ1" Grid.Row="3"/>
                            <Label Content="アクセ2" Grid.Row="3" Grid.Column="2"/>
                            <ComboBox Grid.Row="1" Grid.Column="1" ItemsSource="{Binding ElementName=ListBoxCharItem, Path=Items}" DisplayMemberPath="Name" SelectedIndex="{Binding ElementName=ListBoxChar, Path=SelectedItem.EquipRightHand}"/>
                            <ComboBox Grid.Row="1" Grid.Column="3" ItemsSource="{Binding ElementName=ListBoxCharItem, Path=Items}" DisplayMemberPath="Name" SelectedIndex="{Binding ElementName=ListBoxChar, Path=SelectedItem.EquipLeftHand}"/>
                            <ComboBox Grid.Row="2" Grid.Column="1" ItemsSource="{Binding ElementName=ListBoxCharItem, Path=Items}" DisplayMemberPath="Name" SelectedIndex="{Binding ElementName=ListBoxChar, Path=SelectedItem.EquipHead}"/>
                            <ComboBox Grid.Row="2" Grid.Column="3" ItemsSource="{Binding ElementName=ListBoxCharItem, Path=Items}" DisplayMemberPath="Name" SelectedIndex="{Binding ElementName=ListBoxChar, Path=SelectedItem.EquipBody}"/>
                            <ComboBox Grid.Row="3" Grid.Column="1" ItemsSource="{Binding ElementName=ListBoxCharItem, Path=Items}" DisplayMemberPath="Name" SelectedIndex="{Binding ElementName=ListBoxChar, Path=SelectedItem.EquipAccessory1}"/>
                            <ComboBox Grid.Row="3" Grid.Column="3" ItemsSource="{Binding ElementName=ListBoxCharItem, Path=Items}" DisplayMemberPath="Name" SelectedIndex="{Binding ElementName=ListBoxChar, Path=SelectedItem.EquipAccessory2}"/>
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="れんけい・スキル">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="13*"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <ListBox x:Name="ListBoxTechnique"/>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Button x:Name="ButtonTechniqueCheck">
                            <Image Source="{StaticResource Check}"/>
                        </Button>
                        <Button x:Name="ButtonTechniqueUnCheck" Grid.Column="1">
                            <Image Source="{StaticResource UnCheck}"/>
                        </Button>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="ふくろ">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <DockPanel>
                        <Label Content="どうぐぶくろ" DockPanel.Dock="Top"/>
                        <ComboBox x:Name="ComboBoxBagToolPage"  DockPanel.Dock="Top"/>
                        <StackPanel x:Name="StackPanelBagTool" Margin="0,5,0,0"/>
                    </DockPanel>
                    <DockPanel Grid.Column="2">
                        <Label Content="そうびぶくろ" DockPanel.Dock="Top"/>
                        <ComboBox x:Name="ComboBoxBagEquipmentPage"  DockPanel.Dock="Top"/>
                        <StackPanel x:Name="StackPanelBagEquipment" Margin="0,5,0,0"/>
                    </DockPanel>
                    <StackPanel Grid.Column="4">
                        <Label Content="■ふくろの種類"/>
                        <RadioButton x:Name="RadioButtonBagTypeTool" Content="どうぐ" GroupName="Type"/>
                        <RadioButton x:Name="RadioButtonBagTypeEquipment" Content="そうび" GroupName="Type"/>
                        <Label Content="■何をする？"/>
                        <RadioButton x:Name="RadioButtonBagChangeCount" Content="個数変更" GroupName="Action"/>
                        <RadioButton x:Name="RadioButtonBagClear" Content="全て削除" GroupName="Action"/>
                        <RadioButton x:Name="RadioButtonBagAppend" Content="全て追加" GroupName="Action"/>
                        <StackPanel Orientation="Horizontal">
                            <Label Content="個数"/>
                            <TextBox x:Name="TextBoxBagItemCount" Text="1"/>
                            <Label Content="個"/>
                        </StackPanel>
                        <Button x:Name="ButtonBagChange" Content="実行" Click="ButtonBagChange_Click"/>
                    </StackPanel>
                </Grid>
            </TabItem>
            <TabItem Header="だいじなもの">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="13*"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <ListBox x:Name="ListBoxImportant"/>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Button x:Name="ButtonImportantCheck">
                            <Image Source="{StaticResource Check}"/>
                        </Button>
                        <Button x:Name="ButtonImportantUnCheck" Grid.Column="1">
                            <Image Source="{StaticResource UnCheck}"/>
                        </Button>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="レシピ">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="13*"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <ListBox x:Name="ListBoxRecipe"/>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Button x:Name="ButtonRecipeCheck">
                            <Image Source="{StaticResource Check}"/>
                        </Button>
                        <Button x:Name="ButtonRecipeUnCheck" Grid.Column="1">
                            <Image Source="{StaticResource UnCheck}"/>
                        </Button>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="称号">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="13*"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <ListBox x:Name="ListBoxTitle"/>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Button x:Name="ButtonTitleCheck">
                            <Image Source="{StaticResource Check}"/>
                        </Button>
                        <Button x:Name="ButtonTitleUnCheck" Grid.Column="1">
                            <Image Source="{StaticResource UnCheck}"/>
                        </Button>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="クエスト">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="13*"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <ListBox x:Name="ListBoxQuest"/>
                    <StackPanel Grid.Row="1" Orientation="Horizontal">
                        <Label Content="全て" VerticalAlignment="Center"/>
                        <ComboBox x:Name="ComboBoxQuestState" Width="100"  VerticalAlignment="Center">
                            <ComboBoxItem Content="詳細不明"/>
                            <ComboBoxItem Content="詳細判明"/>
                            <ComboBoxItem Content="受注中"/>
                            <ComboBoxItem Content="達成"/>
                            <ComboBoxItem Content="クリア"/>
                            <ComboBoxItem Content="見えない"/>
                            <ComboBoxItem Content="不定"/>
                        </ComboBox>
                        <Label Content="に" VerticalAlignment="Center"/>
                        <Button x:Name="ButtonQuestPatch" Content="設定"/>
                    </StackPanel>
                </Grid>
            </TabItem>
            <TabItem Header="ルーラ">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="13*"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <ListBox x:Name="ListBoxZoom"/>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Button x:Name="ButtonZoomCheck">
                            <Image Source="{StaticResource Check}"/>
                        </Button>
                        <Button x:Name="ButtonZoomUnCheck" Grid.Column="1">
                            <Image Source="{StaticResource UnCheck}"/>
                        </Button>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="パーティー">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition Height="12*"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Label Content="一覧"/>
                    <Label Content="対象" Grid.Column="1"/>
                    <ListBox x:Name="ListBoxParty" Grid.Row="1" ItemsSource="{Binding Party.List}" DisplayMemberPath="Name"/>
                    <Grid Grid.Row="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Button x:Name="ButtonPartyUp" Click="ButtonPartyUp_Click">
                            <Image Source="{StaticResource Arrow}"/>
                        </Button>
                        <Button x:Name="ButtonPartyDown" Grid.Column="1" RenderTransformOrigin="0.5, 0.5" Click="ButtonPartyDown_Click">
                            <Button.RenderTransform>
                                <ScaleTransform ScaleY="-1"/>
                            </Button.RenderTransform>
                            <Image Source="{StaticResource Arrow}"/>
                        </Button>
                        <Button x:Name="ButtonPartyAppend" Grid.Column="2" Click="ButtonPartyAppend_Click">
                            <Image Source="{StaticResource Append}"/>
                        </Button>
                        <Button x:Name="ButtonPartyRemove" Grid.Column="3" Click="ButtonPartyRemove_Click">
                            <Image Source="{StaticResource Remove}"/>
                        </Button>
                    </Grid>
                    <StackPanel Grid.Row="1" Grid.Column="1">
                        <ComboBox ItemsSource="{Binding ElementName=ListBoxChar, Path=Items}" DisplayMemberPath="Name" SelectedIndex="{Binding ElementName=ListBoxParty, Path=SelectedItem.ID}"/>
                    </StackPanel>
                </Grid>
            </TabItem>
            <TabItem Header="モンスター図鑑">
                <DockPanel>
                    <StackPanel Orientation="Horizontal" DockPanel.Dock="Bottom">
                        <RadioButton x:Name="RadioButtonAll" VerticalAlignment="Center" IsChecked="True" Content="全て"/>
                        <RadioButton x:Name="RadioButtonNone" VerticalAlignment="Center" Content="討伐数0"/>
                        <RadioButton x:Name="RadioButtonHave" VerticalAlignment="Center" Content="討伐数0以外"/>
                        <TextBox x:Name="TextBoxMonsterCount" Margin="10,0,0,0" Width="100" Text=""/>
                        <Button x:Name="ButtonMonsterDecision" Content="に設定"/>
                    </StackPanel>
                    <ScrollViewer>
                        <StackPanel x:Name="StackPanelMonster"/>
                    </ScrollViewer>
                </DockPanel>
            </TabItem>
            <TabItem Header="ストーリー">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="13*"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <ListBox x:Name="ListBoxStory"/>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Button x:Name="ButtonStoryCheck">
                            <Image Source="{StaticResource Check}"/>
                        </Button>
                        <Button x:Name="ButtonStoryUnCheck" Grid.Column="1">
                            <Image Source="{StaticResource UnCheck}"/>
                        </Button>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="ヨッチ族">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition/>
