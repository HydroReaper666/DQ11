                        <ComboBoxItem Content="未チャレンジ"/>
                        <ComboBoxItem Content="初回クリア"/>
                        <ComboBoxItem Content="25手以内"/>
                        <ComboBoxItem Content="20手以内"/>
                        <ComboBoxItem Content="15手以内"/>
                    </ComboBox>
                    <Label Grid.Column="4" Content="四の試練"/>
                    <TextBox Grid.Row="1" Grid.Column="4" Text="{Binding TrialFourScore}"/>
                    <ComboBox Grid.Row="2" Grid.Column="4" SelectedIndex="{Binding TrialFourStep}">
                        <ComboBoxItem Content="未チャレンジ"/>
                        <ComboBoxItem Content="初回クリア"/>
                        <ComboBoxItem Content="30手以内"/>
                        <ComboBoxItem Content="25手以内"/>
                        <ComboBoxItem Content="20手以内"/>
                    </ComboBox>
                    <Label Grid.Column="5" Content="最終試練"/>
                    <TextBox Grid.Row="1" Grid.Column="5" Text="{Binding TrialFinalScore}"/>
                    <ComboBox Grid.Row="2" Grid.Column="5" SelectedIndex="{Binding TrialFinalStep}">
                        <ComboBoxItem Content="未チャレンジ"/>
                        <ComboBoxItem Content="初回クリア"/>
                        <ComboBoxItem Content="40手以内"/>
                        <ComboBoxItem Content="35手以内"/>
                        <ComboBoxItem Content="30手以内"/>
                    </ComboBox>
                </Grid>
