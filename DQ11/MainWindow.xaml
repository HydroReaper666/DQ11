            <TabItem Header="大修練場">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition Height="12*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Row="1" Content="最速スコア"/>
                    <Label Grid.Row="2" Content="ごほうび"/>
                    <Label Grid.Column="1" Content="壱の試練"/>
                    <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding TrialFirstScore}"/>
                    <ComboBox Grid.Row="2" Grid.Column="1" SelectedIndex="{Binding TrialFirstStep}">
                        <ComboBoxItem Content="未チャレンジ"/>
                        <ComboBoxItem Content="初回クリア"/>
                        <ComboBoxItem Content="16手以内"/>
                        <ComboBoxItem Content="12手以内"/>
                        <ComboBoxItem Content="8手以内"/>
                    </ComboBox>
                    <Label Grid.Column="2" Content="弐の試練"/>
                    <TextBox Grid.Row="1" Grid.Column="2" Text="{Binding TrialSecondScore}"/>
                    <ComboBox Grid.Row="2" Grid.Column="2" SelectedIndex="{Binding TrialSecondStep}">
                        <ComboBoxItem Content="未チャレンジ"/>
                        <ComboBoxItem Content="初回クリア"/>
                        <ComboBoxItem Content="20手以内"/>
                        <ComboBoxItem Content="15手以内"/>
                        <ComboBoxItem Content="10手以内"/>
                    </ComboBox>
                    <Label Grid.Column="3" Content="参の試練"/>
                    <TextBox Grid.Row="1" Grid.Column="3" Text="{Binding TrialThirdScore}"/>
