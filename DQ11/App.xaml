             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DQ11"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="Open" UriSource="./Resource/Open.png"/>
